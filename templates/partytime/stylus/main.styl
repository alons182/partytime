	&:nth-child(3n+0)
		background-color fusia
		border-color fusia
	&:nth-child(4n+0)
		background-color amarillo
		border-color amarillo
	
	
	&:hover
		background-color cafe
		border-color cafe
		
	.page-header
	span
		display block
		color #FFF 
		text-align center
		

	img 
		width 100%
		height auto
.span3
	.item 
		color #FFF	
		h2 
			font-weight normal
			margin-top 0
			margin-bottom 5px
		a 
			text-decoration none 
			color #FFF
		p 
			font-family 'museo_sans500'
			font-size 13px
			margin-top 0
			margin-bottom 5px
			height 25px
			overflow hidden
	.pull-left
		img 
			border none
			padding 0
			border-radius 0
	.readmore
		background verde
		text-align center

	&:nth-child(3n+0)
		background-color fusia
		border-color fusia
	&:nth-child(4n+0)
		background-color amarillo
		border-color amarillo


.menu-footer
	
	li 
		margin .5em
		a 
			color #FFF 
			text-decoration none
			padding .5em
