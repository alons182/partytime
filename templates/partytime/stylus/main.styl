	width 350px
	margin-right 10px
	overflow hidden
	
	
	img 
		width 100% !important
		padding .2em
		border 1px solid #EFEFF0
		border-radius 3px
		box-sizing border-box
		background #FFF
		//transition all .2s ease
		/*&:hover
			transform scale(1.06)*/


.itemContentBlock
	width 100% //600px

.itemRatingBlock
	padding 8px 0
	span 
		display block
		float left
		font-style normal
		padding 0 4px 0 0
		margin 0
		color #999
.itemRatingForm
	display block
	vertical-align middle
	line-height 25px
	float left

.itemRatingList,
.itemRatingList a:hover,
.itemRatingList .itemCurrentRating 
	background: rgba(0, 0, 0, 0) url(../img/transparent_star.gif) left -1000px repeat-x;

.itemRatingList
	position relative
	float left
	width 125px
	height 25px
	overflow hidden
	list-style none
	margin 0
	padding 0
	background-position left top
	
	
	li 
		display inline
		background none
		padding 0

	.one-star
		width 20%
		z-index 6
	.two-star
		width 40%
		z-index 5
	.three-stars
		width 60%
		z-index 4
	.four-stars
		width 80%
		z-index 3
	.five-stars 
		width 100%
		z-index 2

	a
	.itemCurrentRating
		position: absolute;
		top: 0;
		left: 0;
		text-indent: -1000px;
		height: 25px;
		line-height: 25px;
		outline: none;
		overflow: hidden;
		border: none;
		cursor: pointer;

	.itemCurrentRating
		z-index: 1;
		background-position: 0 center;
		margin: 0;
		padding: 0;
	a
		&:hover
			background-position:left bottom



.itemRatingLog 
