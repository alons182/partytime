@import "nib"
@import "normalize.styl"
@import "icons.styl"

@font-face {
	font-family: 'museo_sans500';
	src: url('../fonts/museosans_500-webfont.eot');
	src: url('../fonts/museosans_500-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/museosans_500-webfont.woff') format('woff'),
		 url('../fonts/museosans_500-webfont.ttf') format('truetype'),
		 url('../fonts/museosans_500-webfont.svg#museo_sans500') format('svg');
	font-weight: normal;
	font-style: normal;

}

@font-face {
	font-family: 'museo_sans900';
	src: url('../fonts/museosans_900-webfont.eot');
	src: url('../fonts/museosans_900-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/museosans_900-webfont.woff') format('woff'),
		 url('../fonts/museosans_900-webfont.ttf') format('truetype'),
		 url('../fonts/museosans_900-webfont.svg#museo_sans900') format('svg');
	font-weight: normal;
	font-style: normal;

}


@font-face {
	font-family: 'museo_sans300';
	src: url('../fonts/museosans-300-webfont.eot');
	src: url('../fonts/museosans-300-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/museosans-300-webfont.woff') format('woff'),
		 url('../fonts/museosans-300-webfont.ttf') format('truetype'),
		 url('../fonts/museosans-300-webfont.svg#museo_sans300') format('svg');
	font-weight: normal;
	font-style: normal;

}

fusia = #ee2b9c
azul = #19add6
amarillo = #ffba00 
cafe = #5c2a0f
verde = #b1d13a 

body
	font-family 'museo_sans500', serif
	font-size 16px
	line-height normal
	line-height 25px
	color #898989
	background url('../img/circulos2.png'), url('../img/bg.jpg')  center top repeat
	//background-color #00a6b9

ul 
	margin 0
	padding 0
	list-style none

header
	position relative
	clearfix()
	

figure
	margin 0
	padding 0
h1
	font-family 'Chewy', cursive
	color fusia
	font-weight normal
h2
	font-family 'Chewy', cursive
	font-weight normal

a 
	text-decoration none
	color fusia		

footer
	
	height 200px
	position relative
	margin-top 50px
	padding
	clearfix() 

	h3 
		color #FFF
		font-size 1em
		font-family 'museo_sans900'
		

	&:after
		content ""
		position absolute
		display block
		width 100%
		height 5px
		background url('../img/bottom-wave.png') repeat-x
		z-index 2
		top 0px


.fusia
	background fusia
.azul 
	background azul
.amarillo 
	background amarillo
.cafe 
	background cafe
.btn
	display inline-block
	height 34px
	padding 0 12px
	color #FFF 
	font-size 0.9em 
	line-height 36px
	text-align center
	letter-spacing 1px
	background-color azul
	border none
	outline none
	border-radius 2px
	cursor pointer
	text-decoration none
	vertical-align middle
	

.btn-success
.btn-primary
	background-color fusia !important
	color #FFF
	&:hover
		background-color azul !important 

.logo
	display block
	width 150px
	margin 0 auto
	
	img 
		width 100%
.redes
	text-align center
	margin-bottom 10px
   
	a 
		display inline-block
		text-decoration none
		color #fff
		background fusia
		
		padding .5em
		&:hover
			background azul
   

#btn_mobile
	float left
	
	.icon-menu 
		font-size 2em
		color #FFF
		padding .5em
.cart 
	float right
	font-size 13px
	color #FFF
	background verde
	padding 1em
	text-align center
	border-radius 50%
	width 25% 
	position absolute
	right 0
	top 29px 

.search
	float left
	width 50%
	z-index 20


	.finder	
	.inner-search
