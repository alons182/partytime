.inputbox
	background #fff
	border none
	border-radius 5px
	border 1px solid #c2c2c2
	color azul
	font-size 14px
	padding 8px 8px 8px 8px

.globos
	width 109px
	height 155px
	display none
	overflow hidden
	background url('../img/globos.png') no-repeat 
	animation floating2 5s infinite alternate
	position absolute
	top 32%
	left 17%
	
.cloud
	width 287px
	height 193px
	display none
	overflow hidden
	background url('../img/clouds-blue-sprite.png') no-repeat -36px -290px
	animation floating2 10s infinite alternate
	position absolute
	top 3%
	left 0%
	
	.info
		display block
		position relative
		top 70px
		left 0px
		font-size 1.2em
		font-family: 'Chewy', cursive;
		color fusia
		text-align center
	

.menu
	display none
	margin-top 50px   
	ul 
		clearfix()
	li 
		display block

		.nav-header
		a 
			display block
			text-decoration none 
			padding 1em
			color #FFF 
			font-family 'Chewy', cursive
			font-size 16px
			text-transform uppercase
			text-align center
			letter-spacing 2px
			text-shadow 1px 1px 1px rgba(0,0,0,.5)
			transition all .2s ease
			cursor pointer
			line-height normal

	
		.nav-child
		.submenu
			//display none
			//position absolute
			width 100%
			left 0
			transition all .3s ease
		   
	
	

.slider
	display none
	margin-top 30px
	min-height 300px 
	border 5px solid azul
	border-radius 5px
	img
		width 100%



.wrapper
	width auto
	margin 0 auto
	background #FFF
	box-shadow 0px 0px 3px rgba(0,0,0,.5)
	border-radius 5px 5px 0 0
.main 
	margin-top 30px

.categories-list
	&>div
		display block

		&:nth-child(3n+0)
			a
				background-color fusia
				border-color fusia
		&:nth-child(4n+0)
			a
				background-color amarillo
				border-color amarillo
.categories
	padding 0 1em
	text-align center
	margin-top 20px
	padding-top 10px
	

		
	.category-desc
		display none

.top-sellers
	padding 0 1em
	position relative

	.slide
	li 
		//width 7em
		margin 0 1em


	img 
		width 100%
		height auto
		border 5px solid azul
		border-radius 5px
.actions
	position absolute
	text-decoration none
	color #fff
	top 60%
	font-size 1em
	background fusia
	padding .5em
	box-shadow 1px 1px 1px rgba(0,0,0,.8)

	&:hover
		background azul

#prev
	left 0
#next
	right 0
	
.span3		
.category
	display block
	background-color azul
	margin-bottom .5em
	border 5px solid azul
	text-decoration none
	transition all .5s ease
	box-shadow 1px 1px 1px rgba(0,0,0,.8)
	border-radius 5px 5px 0 0
